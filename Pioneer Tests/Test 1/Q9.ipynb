    "import sympy as sm\n",
    "t = sm.symbols('t')\n",
    "u = sm.Heaviside\n",
    "y1 = t*u(t)-(t-3)*u(t-3)-(t-5)*u(t-5)+(t-8)*u(t-8)\n",
    "x1 =2*u(t)-u(t-3)-u(t-5)\n",
    "x2 = 4*u(t-3)-2*u(t-6)-2*u(t-8)\n",
    "\n",
    "\"\"\"\n",
    "x2 = 2*u(t-3)\n",
    ": y2 = 2y1(t-3)\n",
    "\"\"\"\n",
    "y2 = 2*y1.subs(t, t-3)\n",
    "y2.subs(t, 6)"
